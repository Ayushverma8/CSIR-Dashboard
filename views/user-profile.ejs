                                            <th> Qualified</th>
                                            <th> Authority</th>
                                            <th> Specialization</th>
                                            <th>Score</th>
                                            <th>Year Appreared</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% for (var i = 0; i < ExamsAndExtras.length; i++) { %>
                                        <tr>
                                            <td><%= ExamsAndExtras[i].GATENET_EXAM %></td>
                                            <td><%= ExamsAndExtras[i].GATENET_Authority %></td>
                                            <td><%= ExamsAndExtras[i].GATENET_Specialization %></td>
                                            <td><%= ExamsAndExtras[i].GATENET_ScoreObtained %></td>
                                            <td><%= ExamsAndExtras[i].GATENET_YearAppeared %></td>

                                        </tr>
                                        <% } %>


                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                    <!-- End Example Responsive -->


                </div>
            </div>
