                                            <th>Organisation</th>
                                            <th>Place</th>
                                            <th>FromDate</th>
                                            <th>ToDate</th>
                                            <th>PositionHeld</th>
                                            <th>AreaOfWork</th>
                                            <th>Sector</th>

                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% for (var i = 0; i < EmploymentHistory.length; i++) { %>
                                        <tr>
                                            <td><%= EmploymentHistory[i].Organisation %></td>
                                            <td><%= EmploymentHistory[i].Place %></td>
                                            <td><%= EmploymentHistory[i].FromDate %></td>
                                            <td><%= EmploymentHistory[i].ToDate %></td>
                                            <td><%= EmploymentHistory[i].PositionHeld %></td>
                                            <td><%= EmploymentHistory[i].AreaOfWork %></td>
                                            <td><%= EmploymentHistory[i].Sector %></td>


                                        </tr>
                                        <% } %>


                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                    <!-- End Example Responsive -->


                </div>
            </div>
            <!-- Panel Table Example1 -->
            <div class="panel">
                <div class="panel-body">
                    <!-- Example Responsive -->
