    <!-- Page -->
    <div class="page">
        <div class="page-header">
            <h1 class="page-title">Application Number : <%= ApplicationNumber %> ✅ Applied on : <%= Timestamp %></h1>
            <div class="page-header-actions">
                
            </div>
        </div>

        <div class="page-content">
            <!-- Panel -->
            <div class="panel">
                <div class="panel-body container-fluid">
                    <div class="row row-lg">
                        <div class="col-lg-6">
                            <!-- Example Basic -->
                            <div class="example-wrap">
                                <h4 class="example-title">Basic details</h4>

                                <div class="example table-responsive">
                                    <table class="table">
                                        <thead>
                                            <tr>
                                                <th>#</th>
                                                <th>Field</th>
                                                <th>Value</th>

                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                                <td>1</td>
                                                <td>Name</td>
                                                <td><%= Name %></td>


                                            </tr>
                                            <tr>
                                                <td>2</td>
                                                <td>Father's Name</td>
                                                <td><%= FAName %></td>

                                            </tr>
                                            <tr>
                                                <td>3</td>
                                                <td>Gender</td>
                                                <td><%= Gender %></td>

                                            </tr>
                                            <tr>
                                                <td>4</td>
                                                <td>Category</td>
                                                <td><%= Category %></td>

                                            </tr>
                                            <tr>
                                                <td>5</td>
                                                <td>Nationality</td>
                                                <td><%= Nationality %></td>

                                            </tr>
                                            <tr>
                                                <td>6</td>
                                                <td>DOB</td>
                                                <td><%= DOB %></td>

                                            </tr>
                                            <tr>
                                                <td>7</td>
                                                <td>Email</td>
                                                <td><%= Email %></td>

                                            </tr>
                                            <tr>
                                                <td>8</td>
                                                <td>Phone Number</td>
                                                <td><%= PhoneNumber %></td>

                                            </tr>
                                            <tr>
                                                <td>9</td>
                                                <td>Address</td>
                                                <td><%= Address %></td>

                                            </tr>
                                            <tr>
                                                <td>10</td>
                                                <td>Skype</td>
                                                <td><%= SkypeID %></td>

                                            </tr>
                                            <tr>
                                                <td>11</td>
                                                <td>Zoom</td>
                                                <td><%= ZoomID %></td>

                                            </tr>
                                            <tr>
                                                <td>12</td>
                                                <td>Whatsapp Number</td>
                                                <td><%= WhatsAppNumber %></td>

                                            </tr>
                                            <tr>
                                                <td>13</td>
                                                <td>PWD Status</td>
                                                <td><%= PWDStatus %></td>

                                            </tr>
                                            <tr>
                                                <td>14</td>
                                                <td>Application Number</td>
                                                <td><%= ApplicationNumber %></td>

                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                            <!-- End Example Basic -->
                        </div>

                        <div class="col-lg-6">
                            <!-- Example Hover Table -->
                            <div class="example-wrap">
                                <h4 class="example-title">Project Details</h4>
                                <div class="example table-responsive">
                                    <table class="table table-hover">
                                        <thead>
                                            <tr>
                                                <th>#</th>
                                                <th>Field</th>
                                                <th>Value</th>

                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
